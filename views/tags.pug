extends header
include  mixins/_storeCard

block content
    .inner
        h2.f2 #{tag || 'TAGS'}
        each t in tags
            a.mr4(href=`/tags/${t._id}`) ##{t._id}
                span= ' ' + t.count
        .flex.flex-wrap.justify-between
            each store in stores
                +storeCard(store)


